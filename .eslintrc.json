{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "plugin:vue-a11y/recommended"
  ],
  "rules": {
    "no-param-reassign": [
      "error", 
      { 
        "props": false
      }
    ]
  }
}
